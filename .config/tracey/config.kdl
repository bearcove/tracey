spec {
    name "tracey"
    prefix "r"
    source_url "https://github.com/bearcove/tracey"
    include "docs/spec/**/*.md"

    impl {
        name "main"
        include "crates/**/*.rs"
        include "crates/**/*.ts"
        include "crates/**/*.tsx"
        include "crates/**/*.css"
        include "tracey-zed/src/**/*.rs"
        include "../marq/**/*.rs"
        exclude "target/**"
        exclude "../marq/target/**"
    }
}
